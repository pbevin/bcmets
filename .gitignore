# Ignore bundler config
/.bundle
# ...and locally installed gems
/vendor/bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/sphinx

# Ignore all logfiles and tempfiles.
/log
/tmp
/arch
.env

*.swp
.secret
public/system
